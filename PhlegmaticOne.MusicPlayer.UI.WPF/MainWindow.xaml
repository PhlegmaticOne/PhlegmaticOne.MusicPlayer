<Window x:Class="PhlegmaticOne.MusicPlayer.UI.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PhlegmaticOne.MusicPlayer.UI.WPF"
        xmlns:controls="clr-namespace:PhlegmaticOne.MusicPlayer.UI.WPF.Controls"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" WindowStyle="None">
    <DockPanel>
        <controls:Navigation DockPanel.Dock="Left" DataContext="{Binding Navigator}"/>
        <Grid>
            <ContentPresenter Content="{Binding Navigator.CurrentViewModel}"></ContentPresenter>
        </Grid>
    </DockPanel>
</Window>
